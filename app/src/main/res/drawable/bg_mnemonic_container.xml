<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape>
            <solid android:color="#29346100" />
            <stroke
                android:color="#B8346100"
                android:dashGap="5dp"
                android:dashWidth="7dp"
